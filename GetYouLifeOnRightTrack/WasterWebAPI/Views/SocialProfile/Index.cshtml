@model WasterWebAPI.Models.SocialProfileModel[]

@{
    ViewBag.Title = "Users";
}

<h2>@ViewBag.Title</h2>

<table>
    <thead>
        <tr>
            <th>
                First name
            </th>
            <th>
                Last Name
            </th>
            <th>
                Nickname
            </th>
            <th>
                Photo
            </th>
            <th>
                Typ
            </th>
        </tr>
    </thead>
    @foreach (var record in Model)
    {
        <tr>
            <td>
                @record.FirstName
            </td>
            <td>
                @record.LastName
            </td>
            <td>
                @record.Nickname
            </td>
            <td>
                <img src="@record.PhotoUrl" alt="@record.Nickname" />
            </td>
        </tr>

    }
</table>
